[Unit]
Description = Start Jupyter notebook server after reboot
After = multi-user.target

#[Timer]
#OnBoot = 30

[Service]
Type=simple
User=pi
Group=pi
WorkingDirectory = /home/pi/notebooks
ExecStart=/home/pi/berryconda3/bin/jupyter notebook
Restart=always
RestartSec=10

[Install]
WantedBy = multi-user.target
